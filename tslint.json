{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-microsoft-contrib",
    "tslint-consistent-codestyle"
  ],
  "rules": {
    "align": false,
    "arrow-return-shorthand": [true, "multiline"],
    "completed-docs": false,
    "export-name": false,
    "import-name": false,
    "insecure-random": false,
    "interface-name": [true, "never-prefix"],
    "jsx-no-multiline-js": false,
    "jsx-no-lambda": false,
    "linebreak-style": false,
    "match-default-export-name": false,
    "max-line-length": [
      true,
      {
        "limit": 120,
        "ignore-pattern": "^import"
      }
    ],
    "missing-jsdoc": false,
    "mocha-no-side-effect-code": false,
    "newline-before-return": false,
    "newline-per-chained-call": false,
    "no-angle-bracket-type-assertion": true,
    "no-any": false,
    "no-backbone-get-set-outside-model": false,
    "no-console": [true],
    "no-default-export": false,
    "no-empty": false,
    "no-empty-interface": false,
    "no-http-string": false,
    "no-implicit-dependencies": false,
    "no-import-side-effect": false,
    "no-null-keyword": false,
    "no-non-null-assertion": false,
    "no-unnecessary-else": true,
    "no-relative-imports": false,
    "no-reserved-keywords": false,
    "no-require-imports": false,
    "no-single-line-block-comment": false,
    "no-string-throw": true,
    "no-submodule-imports": false,
    "no-unnecessary-callback-wrapper": false,
    "no-unsafe-any": false,
    "no-use-before-declare": false,
    "no-var-requires": false,
    "non-literal-require": false,
    "number-literal-format": false,
    "object-literal-shorthand": true,
    "ordered-imports": [
      true,
      {
        "grouped-imports": true,
        "import-sources-order": "lowercase-first",
        "named-imports-order": "lowercase-first",
        "module-source-path": "full"
      }
    ],
    "possible-timing-attack": false,
    "prefer-const": true,
    "prefer-type-cast": false,
    "promise-function-async": false,
    "quotemark": [true, "single", "jsx-double", "avoid-escape"],
    "react-a11y-anchors": false,
    "react-a11y-role-has-required-aria-props": false,
    "react-a11y-input-elements": false,
    "react-a11y-no-onchange": false,
    "react-anchor-blank-noopener": false,
    "react-iframe-missing-sandbox": false,
    "react-this-binding-issue": false,
    "react-tsx-curly-spacing": [
      true,
      "never",
      {
        "allowMultiline": true
      }
    ],
    "semicolon": [true, "always", "ignore-bound-class-methods"],
    "function-name": [
      true,
      {
        "static-method-regex": "^(getDerivedStateFromProps)|([A-Z][\\w\\d]+)$"
      }
    ],
    "strict-boolean-expressions": false,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ],
    "typedef": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ]
  }
}
